---
- name: Copiar Archivos
  hosts: all
  become: yes
  tasks:
    - name: copy nuevahora
      ansible.builtin.copy:
        src: /tmp/nuevahora
        dest: /home/ansible/
        owner: ansible
        mode: '0644'
    - name: Check system information
      shell: zic nuevahora
      register: horanew
    - debug: var=horanew.stdout_lines
